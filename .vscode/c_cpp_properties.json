{
    "configurations": [
        {
            "name": "ARM-Cortex-M",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/../microchip-asf/sam0/**"
            ],
            "defines": [
                "BOARD=SAMD20_XPLAINED_PRO",
                "__SAMD20J18__",
                "ARM_MATH_CM0PLUS=true"
            ],
            "compilerPath": "/usr/bin/arm-none-eabi-gcc",
            "cStandard": "c99",
            "cppStandard": "c++98",
            "intelliSenseMode": "gcc-arm",
            "compilerArgs": [
                "-mcpu=cortex-m0plus",
                "-mthumb"
            ]
        }
    ],
    "version": 4
}